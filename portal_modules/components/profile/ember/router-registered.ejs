window.Ember.$.getScript('<%= mountPath %>/logoutComponent');

define(
	"twyrPortal/routes/profile",
	["exports"],
	function(exports) {
		if(window.developmentMode) console.log('DEFINE: twyrPortal/routes/profile');
		var ManageProfileRoute = window.Ember.Route.extend({
			'beforeModel': function() {
				return window.Ember.$.getScript('<%= mountPath %>/mvc');
			},

			'model': function() {
				return this.store.find('profile', '<%= userId %>');
			},

			'setupController': function(controller, model) {
				controller.set('model', model);
			}
		});

		exports['default'] = ManageProfileRoute;
	}
);
