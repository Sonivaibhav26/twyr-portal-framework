var Router = require('twyrPortal/router')['default'];
Router.map(function() {
	this.route('profile', { 'path': '/profile' });
});

define(
	"twyrPortal/profile/route",
	["exports"],
	function(exports) {
		if(window.developmentMode) console.log('DEFINE: twyrPortal/profile/route');
		var ManageProfileRoute = window.Ember.Route.extend({
			'beforeModel': function() {
				return window.Ember.$.getScript('<%= mountPath %>/mvc/manageProfile');
			},

			'model': function() {
				return this.store.find('profile', '<%= userId %>');
			}
		});

		exports['default'] = ManageProfileRoute;
	}
);
