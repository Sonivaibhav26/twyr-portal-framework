var Router = require('twyrPortal/router')['default'];
Router.map(function() {
	this.route('organization-manager-organization-structure', { 'path': '/organization/structure' });
	this.route('organization-manager-group-management', { 'path': '/organization/groups' });
});

define(
	"twyrPortal/routes/organization-manager-organization-structure",
	["exports"],
	function(exports) {
		if(window.developmentMode) console.log('DEFINE: twyrPortal/routes/organization-manager-organization-structure');
		var ManageOrganizationManagerOrganizationStructureRoute = window.Ember.Route.extend({
			'beforeModel': function() {
				return window.Ember.$.getScript('<%= mountPath %>/mvc/organizationManagerOrganizationStructure');
			},

			'model': function() {
				return this.store.findRecord('organization-manager-organization-structure', '<%= tenantId %>');
			},

			'setupController': function(controller, model) {
				controller.set('model', model);
			}
		});

		exports['default'] = ManageOrganizationManagerOrganizationStructureRoute;
	}
);

